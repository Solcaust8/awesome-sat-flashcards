<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.SAT.awesomeflashcards"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.SAT.awesomeflashcards.CardStack"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.SAT.awesomeflashcards.Second_Flashcard"
            android:label="@string/title_activity_second__flashcard" >
        </activity>
        <activity
            android:name="com.SAT.awesomeflashcards.Flashcard2"
            android:label="@string/title_activity_flashcard2"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>